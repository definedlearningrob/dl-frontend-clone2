  "scripts": {
    "start": "vite",
    "serve": "vite preview",
    "dc-start": "vite --port 3000",
    "pbl-start": "vite --port 3001",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build-dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build --mode development",
    "dc-preview": "vite preview --port 3000",
    "pbl-preview": "vite preview --port 3001",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=75%",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:ci": "cross-env NODE_ENV=test jest --runInBand --noStackTrace",
    "test:coverage": "jest --watchAll --coverage",
    "cypress:dc-start": "cross-env VITE_DC_API_HOST=http://localhost:4001 VITE_DC_DOMAIN=localhost:3001 vite build && vite preview --port 3001 --host",
    "cypress:dl-start": "cross-env VITE_DL_API_HOST=http://localhost:4001 VITE_PBL_DOMAIN=localhost:3001 vite build && vite preview --port 3001 --host",
    "cypress:dc-start-remote-be": "cross-env VITE_DC_API_HOST=https://e2e-defined-careers-backend.herokuapp.com cross-env VITE_DC_DOMAIN=localhost:3001 vite build && vite preview --port 3001",
    "cypress:dl-start-remote-be": "cross-env VITE_DL_API_HOST=https://e2e-defined-careers-backend.herokuapp.com cross-env VITE_PBL_DOMAIN=localhost:3001 vite build && vite preview --port 3001",
    "cypress:dc-open": "cypress open --config specPattern=cypress/e2e/DC/**/*.e2e.js",
    "cypress:dl-open": "cypress open --config specPattern=cypress/e2e/DL/**/*.e2e.js",
    "cypress:dc-open-remote-be": "cypress_api_host=https://e2e-defined-careers-backend.herokuapp.com cypress open --config specPattern=cypress/e2e/DC/**/*.e2e.js",
    "cypress:dl-open-remote-be": "cypress_api_host=https://e2e-defined-careers-backend.herokuapp.com cypress open --config specPattern=cypress/e2e/DL/**/*.e2e.js",
    "cypress:dc-test": "cypress run --config specPattern=cypress/e2e/DC/**/*.e2e.js",
    "cypress:dl-test": "cypress run --config specPattern=cypress/e2e/DL/**/*.e2e.js",
    "cypress:dc-test-remote-be": "cypress_api_host=https://e2e-defined-careers-backend.herokuapp.com cypress run",
    "cypress:dl-test-remote-be": "cypress_api_host=https://e2e-defined-careers-backend.herokuapp.com cypress run",
    "setup-server-for-e2e-remote": "CI=true vite preview --port 3001",
    "setup-server-for-e2e-remote-dl": "CI=true vite preview --port 3002",
    "setup-server-for-e2e": "npm run start:test & wait-on http://localhost:3001",
    "tsc": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint 'src/**/*.{jsx,js,tsx,ts}'",
    "lint-fix": "tsc --noEmit && eslint 'src/**/*.{jsx,js,tsx,ts}' --fix",
    "lint-staged": "lint-staged --config lint-staged.js  --diff='origin/develop...HEAD'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "analyze": "source-map-explorer 'build/assets/*.js'",
    "release:bump-minor": "npx bumpp minor --no-verify --no-push --no-tag --execute 'npm i'",
    "release:bump-major": "npx bumpp major --no-verify --no-push --no-tag --execute 'npm i'",
    "codegen": "graphql-codegen",
    "postinstall": "node ./postinstall.mjs"
  },
